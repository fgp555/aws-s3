{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "Statement1",
      "Effect": "Allow",
      "Principal": "*",
      "Action": ["s3:GetObject", "s3:DeleteObject", "s3:PutObject"],
      "Resource": "arn:aws:s3:::s3-bucket-tutorial-241223/*"
    },
    {
      "Sid": "Statement2",
      "Effect": "Allow",
      "Principal": "*",
      "Action": "s3:ListBucket",
      "Resource": "arn:aws:s3:::s3-bucket-tutorial-241223"
    }
  ]
}
